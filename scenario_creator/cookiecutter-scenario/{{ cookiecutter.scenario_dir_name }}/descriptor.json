{
  "resources": [
    {
      "engine": "ai_python.exe",
      "entryscript": "main.py",
      "scenario_type": {{ cookiecutter.scenario_type }},
      {{ cookiecutter.supported_platforms }},
      "schema": {
        "type": "object",
        "required": ["first_parameter"],
        "properties": {
          "first_parameter": {
            "title": "First Parameter",
            "type": "string"
          }
        }
      },
      "form": [
        {
          "key": "first_parameter",
          "type": "text",
          "feedback": false,
          "placeholder": "Who do you want to say hello to?",
          "validationMessage": "Please enter a valid string"
        }
      ]
    }
  ]
}